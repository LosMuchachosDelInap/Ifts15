{
    "name": "ifts15/proyecto",
    "description": "Sistema de gestión académica - IFTS15",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "IFTS15 Development Team"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "cloudinary/cloudinary_php": "^3.1",
        "phpmailer/phpmailer": "^6.10",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
